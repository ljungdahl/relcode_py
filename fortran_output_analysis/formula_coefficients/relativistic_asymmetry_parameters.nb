Cell[BoxData[{
 RowBox[{"Clear", "[", "RelativisticAsymmetryParameter", 
  "]"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"Options", "[", "RelativisticAsymmetryParameter", "]"}], "=", 
   RowBox[{"{", 
